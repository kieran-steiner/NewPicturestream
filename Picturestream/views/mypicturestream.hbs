<h2>My Picturestream</h2>
<div class="pictures-container">
  {{#each pictures}}
    <div class="picture-card">
      <img src="/uploads/{{file_name}}" alt="{{title}}" />
      <h3>{{title}}</h3>
      <p>{{description}}</p>
      <p><strong>Hochgeladen von:</strong> {{username}}</p>
      <small>Hochgeladen am {{time}}</small>
    </div>
  {{else}}
    <p>Keine Favoritenbilder gefunden.</p>
  {{/each}}
</div>